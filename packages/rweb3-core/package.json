{
  "name": "rweb3-core",
  "version": "0.0.33",
  "main": "./lib/commonjs/index.js",
  "module": "./lib/esm/index.js",
  "license": "Apache-2.0",
  "keywords": [
    "rigo",
    "rigochain",
    "blockchain",
    "dapp"
  ],
  "homepage": "https://github.com/rigochain/rigo-sdk-js",
  "scripts": {
    "clean": "rimraf dist && rimraf lib",
    "prebuild": "yarn clean",
    "build": "yarn version:output && yarn build:cjs & yarn build:esm & yarn build:types",
    "version:output": "[ -z $npm_package_version ] && rm ./src/version.ts || echo \"/* eslint-disable header/header */ export const Web3PkgInfo = { version: '$npm_package_version' };\" > ./src/version.ts",
    "build:cjs": "tsc --build tsconfig.cjs.json && echo '{\"type\": \"commonjs\"}' > ./lib/commonjs/package.json",
    "build:esm": "tsc --build tsconfig.esm.json && echo '{\"type\": \"module\"}' > ./lib/esm/package.json",
    "build:types": "tsc --build tsconfig.types.json"
  },
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bn.js": "^5.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/keccak": "^3.0.1",
    "@types/long": "^5.0.0",
    "@types/node": "^18.11.18",
    "@types/node-localstorage": "^1.3.0",
    "@types/secp256k1": "^4.0.3",
    "rweb3-providers-http" : "0.0.33",
    "rweb3-validator" : "0.0.33",
    "rweb3-types" : "0.0.33"

  }
}
