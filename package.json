    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "utf-8-validate": "^5.0.9",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
